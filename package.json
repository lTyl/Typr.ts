{
  "name": "typr-ts",
  "version": "1.1.3",
  "description": "Typr.ts - process fonts in TypeScript.",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git:github.com/lTyl/Typr.ts.git"
  },
